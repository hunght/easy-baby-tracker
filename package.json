{
  "name": "native-wind",
  "main": "expo-router/entry",
  "version": "1.0.1",
  "scripts": {
    "start": "kill $(lsof -ti:8081) 2>/dev/null || true && expo start -c",
    "android": "kill $(lsof -ti:8081) 2>/dev/null || true && expo start -c --android",
    "ios": "kill $(lsof -ti:8081) 2>/dev/null || true && expo start -c --ios",
    "web": "expo start -c --web",
    "clean": "rm -rf .expo node_modules",
    "db:generate": "drizzle-kit generate",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{md,json,css,ts,tsx}\"",
    "find-unused": "ts-prune --error",
    "find-unused:detailed": "ts-prune",
    "prepare": "husky",
    "publish": "node scripts/publish.js",
    "publish:android": "node scripts/publish.js --android-only",
    "publish:ios": "node scripts/publish.js --ios-only",
    "publish:build-only": "node scripts/publish.js --skip-submit",
    "publish:submit-only": "node scripts/publish.js --skip-build",
    "generate:icons": "node scripts/generate-icons.js"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.16.1",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/slider": "5.0.1",
    "@react-navigation/native": "^7.0.0",
    "@rn-primitives/avatar": "^1.2.0",
    "@rn-primitives/label": "^1.2.0",
    "@rn-primitives/popover": "^1.2.0",
    "@rn-primitives/portal": "~1.3.0",
    "@rn-primitives/separator": "^1.2.0",
    "@rn-primitives/slot": "^1.2.0",
    "@rn-primitives/tabs": "^1.2.0",
    "@rn-primitives/toggle": "^1.2.0",
    "@rn-primitives/toggle-group": "^1.2.0",
    "@tanstack/react-query": "^5.90.10",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.7",
    "expo": "^54.0.0",
    "expo-auth-session": "~7.0.8",
    "expo-constants": "~18.0.10",
    "expo-drizzle-studio-plugin": "^0.2.1",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-image-picker": "^17.0.8",
    "expo-linking": "~8.0.8",
    "expo-localization": "^17.0.7",
    "expo-notifications": "~0.32.13",
    "expo-router": "~6.0.10",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-sqlite": "~16.0.9",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.8",
    "lucide-react-native": "^0.545.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gifted-charts": "^1.4.68",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@expo/prebuild-config": "^54.0.6",
    "@resvg/resvg-js": "^2.6.2",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9.39.1",
    "eslint-config-expo": "^10.0.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "sharp-cli": "^5.2.0",
    "ts-prune": "^0.10.3",
    "typescript": "~5.9.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{md,json,css}": [
      "prettier --write"
    ]
  },
  "private": true
}
