{
  "name": "easy-baby-tracker",
  "main": "expo-router/entry",
  "version": "1.0.18",
  "scripts": {
    "start": "kill $(lsof -ti:8081) 2>/dev/null || true && expo start -c",
    "android": "kill $(lsof -ti:8081) 2>/dev/null || true && expo start -c --android",
    "ios": "kill $(lsof -ti:8081) 2>/dev/null || true && expo start -c --ios",
    "web": "expo start -c --web",
    "clean": "rm -rf .expo node_modules",
    "convex:dev": "convex dev",
    "convex:deploy": "convex deploy",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{md,json,css,ts,tsx}\"",
    "prepare": "husky",
    "publish:auto": "node scripts/auto-publish.js",
    "publish:auto:no-increment": "node scripts/auto-publish.js --no-increment",
    "screenshots:android": "maestro test .maestro/screenshots-android.yaml --test-output-dir fastlane/metadata/android/en-US/images/phoneScreenshots --flatten-debug-output",
    "screenshots:ios": "maestro test .maestro/screenshots-ios.yaml --test-output-dir fastlane/metadata/ios/screenshots/en-US --flatten-debug-output",
    "screenshots:ipad": "maestro test .maestro/screenshots-ipad.yaml --test-output-dir fastlane/metadata/ios/screenshots/en-US --flatten-debug-output",
    "generate:icons": "node scripts/generate-icons.js"
  },
  "dependencies": {
    "@auth/core": "^0.37.4",
    "@convex-dev/auth": "^0.0.90",
    "@dev-plugins/react-navigation": "^0.4.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/slider": "5.0.1",
    "@react-navigation/native": "^7.0.0",
    "@rn-primitives/label": "^1.2.0",
    "@rn-primitives/portal": "~1.3.0",
    "@rn-primitives/slot": "^1.2.0",
    "@rn-primitives/tabs": "^1.2.0",
    "@rn-primitives/toggle": "^1.2.0",
    "@rn-primitives/toggle-group": "^1.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "convex": "^1.31.3",
    "expo": "~54.0.29",
    "expo-apple-authentication": "^8.0.8",
    "expo-constants": "~18.0.10",
    "expo-crypto": "^15.0.8",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.10",
    "expo-localization": "~17.0.8",
    "expo-notifications": "~0.32.15",
    "expo-router": "~6.0.19",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "lucide-react-native": "^0.545.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gifted-charts": "^1.4.68",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@resvg/resvg-js": "^2.6.2",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.39.1",
    "eslint-config-expo": "^10.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "~5.9.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{md,json,css}": [
      "prettier --write"
    ]
  },
  "private": true
}
